<div class="container">
  <p class="title-md mb"style="color: green"><%= notice %></p>

  <h1 class="title-lg mb">All employees</h1>

  <div id="employees">
    <% @employees.each do |employee| %>
    <div class="mt-ls container">
       <%= render employee %>
    </div>
  <% end %>

  <div class="container mt-ls">
    <%= link_to "New employee", new_employee_path, class: 'button button-main' %>
  </div>
</div>
