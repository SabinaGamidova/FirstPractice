<div id="<%= dom_id employee %>">
  <p>
    <strong>Employee:</strong>
    <%= employee.employee_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= employee.name %>
  </p>

  <p>
    <strong>Surname:</strong>
    <%= employee.surname %>
  </p>

  <p>
    <strong>Patronymic:</strong>
    <%= employee.patronymic %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= employee.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= employee.email %>
  </p>

  <p>
    <strong>Date registration:</strong>
    <%= employee.date_registration %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= employee.position %>
  </p>

  <p>
    <strong>Specialization:</strong>
    <%= employee.specialization %>
  </p>

  <p>
    <strong>Login:</strong>
    <%= employee.login %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= employee.password %>
  </p>

</div>
